        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".feetToMeterConverter.FeetToMeterConverterActivity">
        </activity>

        <activity android:name=".cameraTask.CameraActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".bmiCalculator.BMICalculatorActivity">
        </activity>

        <activity android:name=".tabs.TabsActivity" />
        <activity android:name=".sharedPreferences.SharedPreferencesActivity" />
        <activity android:name=".simpleFragment.MainActivity" />

        <activity
            android:name=".custom_list_view.CustomListViewActivity"
            android:label="Simple ListView" />
        <activity android:name=".sharedPreferences.PreferencesNextActivity" />
        <activity android:name=".sharedPreferences.SharedPreferencesActivity2" />
